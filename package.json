{
  "name": "lamp-server",
  "version": "1.0.0",
  "bin": "build/index.js",
  "dependencies": {
    "ajv": "^8.1.0",
    "async-mutex": "^0.2.4",
    "axios": "^1.7.7",
    "bull": "^3.29.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "ioredis": "^4.22.0",
    "jose": "^5.6.3",
    "mongodb": "^6.8.0",
    "morgan": "^1.9.1",
    "nano": "^10.1.3",
    "nanoid": "^3.1.3",
    "node-fetch": "^2.6.7",
    "rimraf": "^6.0.1",
    "ts-nats": "^1.2.15",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/bson": "^4.2.0",
    "@types/bull": "^3.15.8",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^5.1.0",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^27.5.1",
    "@types/morgan": "^1.9.2",
    "@types/nano": "^7.0.0",
    "@types/node": "^20.19.11",
    "@types/node-fetch": "^2.6.1",
    "@types/qs": "^6.14.0",
    "@types/readable-stream": "^2.3.5",
    "@types/request": "^2.48.4",
    "@types/send": "^0.17.5",
    "@types/serve-static": "^1.15.8",
    "@types/uniqid": "^4.1.3",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^28.1.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.1.10",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.16",
    "ts-jest": "^28.0.2",
    "ts-node": "^9.1.1"
  },
  "overrides": {
    "simple-update-notifier": "2.0.0"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "build": "rimraf build && tsc",
    "coverage": "jest --coverage",
    "dev": "docker compose up -d && nodemon --exec ts-node ./src/index.ts --watch src",
    "start-sw": "dotenv node build/src/index.js",
    "start": "npm run build && npm run start-sw",
    "test": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true
  },
  "prettier": {
    "semi": false,
    "printWidth": 120
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "globalSetup": "./src/test/setup.js"
  },
  "lint-staged": {
    "**/*.ts": [
      "tsc --noEmit"
    ]
  }
}
