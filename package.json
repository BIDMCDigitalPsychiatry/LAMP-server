{
  "name": "lamp-server",
  "version": "1.0.0",
  "bin": "build/app.js",
  "dependencies": {
    "@types/bull": "^3.14.2",
    "@types/node-cache": "^4.2.5",
    "ajv": "^6.10.2",
    "alasql": "^0.4.11",
    "async-mutex": "^0.2.4",
    "aws-sdk": "^2.711.0",
    "body-parser": "^1.19.0",
    "bull": "^3.18.0",
    "class-validator-jsonschema": "^1.3.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "csv-parser": "^2.3.2",
    "dockerode": "^2.5.8",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "fastify": "^2.11.0",
    "fs-extra": "^7.0.1",
    "get-stream": "^5.1.0",
    "jsonata": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mssql": "^6.0.0",
    "nano": "^8.2.2",
    "nanoid": "^3.1.3",
    "node-cache": "^5.1.2",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "openapi3-ts": "^1.3.0",
    "request": "^2.88.0",
    "routing-controllers-openapi": "^1.8.0",
    "s3-proxy": "^1.2.1",
    "sendmail": "^1.6.1",
    "source-map-support": "^0.5.16",
    "tar-stream": "^1.6.2",
    "ts-nats": "^1.2.15",
    "typescript": "^3.7.4",
    "uniqid": "^5.2.0",
    "unzip-stream": "^0.3.0",
    "uuid": "^8.2.0",
    "vhost": "^3.0.2",
    "vm2": "^3.8.4"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/cors": "^2.8.6",
    "@types/crypto-js": "^4.0.1",
    "@types/dockerode": "^2.5.21",
    "@types/express": "^4.17.2",
    "@types/express-http-proxy": "^1.5.12",
    "@types/fs-extra": "^5.1.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.7.37",
    "@types/mssql": "^4.3.1",
    "@types/node": "^13.1.2",
    "@types/node-cron": "^2.0.3",
    "@types/node-fetch": "^2.5.5",
    "@types/readable-stream": "^2.3.5",
    "@types/request": "^2.48.4",
    "@types/tar-stream": "^1.6.1",
    "@types/uniqid": "^4.1.3",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "pkg": "^4.4.2",
    "prettier": "^2.0.5",
    "tsc-watch": "^1.1.39",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1"
  },
  "scripts": {
    "start": "tsc-watch --onSuccess \"node -r source-map-support/register ./build/app.js\"",
    "build": "tsc",
    "deploy": "pkg . -t node10-alpine-x64,node10-linux-x64,node10-macos-x64 --out-path bin/"
  },
  "pkg": {
    "scripts": [
      "build/**/*.js",
      "node_modules/vm2/lib/*.js"
    ],
    "assets": "public/**/*"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "prettier/@typescript-eslint"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "prefer-const": "warn",
          "@typescript-eslint/camelcase": "off",
          "@typescript-eslint/no-use-before-define": "off",
          "@typescript-eslint/no-explicit-any": "off",
          "@typescript-eslint/consistent-type-assertions": "warn"
        }
      }
    ]
  },
  "prettier": {
    "semi": false,
    "printWidth": 120
  }
}
