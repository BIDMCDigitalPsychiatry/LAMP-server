{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "rootDirs": [
        "./src",
        "./test"
    ],
    "outDir": "./build/src",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "inlineSourceMap": true,
    "allowSyntheticDefaultImports": true,
  },
  "files": [
    "src/app.ts",
    "src/applySentry.ts",
    "src/index.ts",
    "src/utils/instrument.ts"
  ]
}